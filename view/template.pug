                .footer-right.col-xs-12.col-sm-6.col-md-5.col-lg-5
                    h3 Newsletter
                    input(type='text' name='name' placeholder='Name' required)
                    input(type='email' name='email' placeholder='Email' required)
                    input(type='submit' value='subscribe now')

            .copyright
